        -o ${WORKSPACE_LOC}/simple_peripheral_cc2640r2lp_oad_offchip_app/FlashROM/${ProjName}_${ConfigName}_oad
        "
        description=""
        launchWizard="false"
        references="simple_peripheral_cc2640r2lp_oad_offchip_stack"
