            -DICALL_LITE
            -DICALL_MAX_NUM_ENTITIES=6
            -DICALL_MAX_NUM_TASKS=3
            -DPOWER_SAVING
            -DRF_SINGLEMODE
            -DUSE_ICALL
            -DTBM_ACTIVE_ITEMS_ONLY
            -Dxdc_runtime_Assert_DISABLE_ALL
            -Dxdc_runtime_Log_DISABLE_ALL
